    "face-api.js": "^0.22.2",
    "lodash": "^4.17.20",
    "multer": "^1.4.2",
    "nuxt": "^2.14.3",
    "nuxt-quasar": "^0.1.3",
    "pm2": "^4.4.1",
    "popper": "^1.0.1",
